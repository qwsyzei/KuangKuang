        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:text="@string/depth"/>
        <TextView
            android:id="@+id/c_result_depth"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"

            android:textColor="@color/black"
