        android:layout_height="wrap_content"
        android:textSize="@dimen/middle_text_size"
        android:layout_marginTop="@dimen/signup__text_m_l_r"
        android:layout_marginBottom="@dimen/login_p_l_r"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:paddingLeft="@dimen/login_p_l_r"
        android:textColor="@color/kuangkuang_yellow"
        android:text="@string/proportion"/>
