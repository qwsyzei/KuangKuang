<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/middle_text_size"
        android:layout_marginTop="@dimen/signup__text_m_l_r"
        android:layout_marginBottom="@dimen/login_p_l_r"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:paddingLeft="@dimen/login_p_l_r"
        android:textColor="@color/kuangkuang_yellow"
        android:text="@string/cut_grade"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:layout_marginRight="@dimen/signup__text_m_l_r"
        android:background="@color/little_gray"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:text="@string/polish"/>
        <TextView
            android:id="@+id/c_result_polish"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:text="@string/symmetry"/>
        <TextView
            android:id="@+id/c_result_symmetry"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/middle_text_size"
        android:layout_marginTop="@dimen/signup__text_m_l_r"
        android:layout_marginBottom="@dimen/login_p_l_r"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:paddingLeft="@dimen/login_p_l_r"
        android:textColor="@color/kuangkuang_yellow"
        android:text="@string/fluorescence"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:layout_marginRight="@dimen/signup__text_m_l_r"
        android:background="@color/little_gray"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:text="@string/fluorescence"/>
        <TextView
            android:id="@+id/c_result_fluorescence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/middle_text_size"
        android:layout_marginTop="@dimen/signup__text_m_l_r"
        android:layout_marginBottom="@dimen/login_p_l_r"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:paddingLeft="@dimen/login_p_l_r"
        android:textColor="@color/kuangkuang_yellow"
        android:text="@string/clarity_characteristics"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:layout_marginRight="@dimen/signup__text_m_l_r"
        android:background="@color/little_gray"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:text="@string/clarity_characteristics"/>
        <TextView
            android:id="@+id/c_result_clarity_characteristics"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/middle_text_size"
        android:layout_marginTop="@dimen/signup__text_m_l_r"
        android:layout_marginBottom="@dimen/login_p_l_r"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:paddingLeft="@dimen/login_p_l_r"
        android:textColor="@color/kuangkuang_yellow"
        android:text="@string/inscription"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/signup__text_m_l_r"
        android:layout_marginRight="@dimen/signup__text_m_l_r"
        android:background="@color/little_gray"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/size_eight"
        android:layout_marginLeft="@dimen/size_eight"
        android:layout_marginRight="@dimen/size_eight"
        android:paddingLeft="@dimen/signup__text_m_l_r"
        android:paddingRight="@dimen/signup__text_m_l_r">

        <TextView
            android:id="@+id/c_result_inscription"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            />
    </LinearLayout>
</LinearLayout>