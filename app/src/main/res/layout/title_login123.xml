<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/title_RelativeLayout"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/login_title_height"
    android:paddingTop="@dimen/login_title_p_t">
    <ImageView
        android:id="@+id/im_back"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/base_margin"
        android:layout_marginLeft="@dimen/base_margin"
        android:clickable="true"
        android:onClick="back"
        android:src="@drawable/background_back"
        />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/login_title_right"
        android:layout_marginRight="@dimen/base_margin"
        android:padding="@dimen/base_margin"
        android:src="@drawable/background_tomain"
        android:clickable="true"
        android:onClick="gotomain"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>